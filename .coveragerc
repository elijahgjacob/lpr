[run]
source = .
omit = 
    tests/*
    .venv/*
    venv/*
    setup.py
    */site-packages/*
    models/*
    videos/*
    results/*
    download_resources.py
    main.py
    __init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

